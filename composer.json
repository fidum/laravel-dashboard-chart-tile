{
    "name": "fidum/laravel-dashboard-chart-tile",
    "description": "Generic chart tiles for laravel dashboard",
    "keywords": [
        "fidum",
        "laravel-dashboard",
        "laravel-dashboard-chart-tile",
        "chart"
    ],
    "homepage": "https://github.com/fidum/laravel-dashboard-chart-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com",
            "homepage": "https://fidum.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "consoletvs/charts": "^6.6.0",
        "spatie/laravel-dashboard": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Fidum\\ChartTile\\": "src",
            "Fidum\\ChartTile\\Examples\\": "examples"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidum\\ChartTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidum\\ChartTile\\ChartTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
